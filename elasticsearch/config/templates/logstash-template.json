{
	"template": "logstash*",
	"mappings": {
		"fluentd": {
			"properties": {
				"@timestamp": {
					"type": "date",
					"format": "dateOptionalTime"
				},
				"log_date": {
					"type": "string"
				},
				"source_host": {
					"type": "ip"
				},
				"message": {
					"type": "string"
				},
				"server_received": {
					"type": "date",
					"format": "yyyy-MM-dd HH:mm:ss Z",
					"locale": "US"
				}
			}
		}
	}
}
